      <cl:group std:sid="group-40b752d5" id="grp2" class="cl-form-group">
        <cl:formdata std:sid="f-data-2bf8cf49" row="1" col="0"/>
        <cl:output std:sid="output-d2cc62f5" value="[placeholder = &quot;파일을 첨부 해주세요&quot;]&#xD;&#xA;1. 파일 선택 전에 &quot;파일을 첨부 해주세요&quot;라는 텍스트가 보인다.">
          <cl:formdata std:sid="f-data-b22c3d7e" row="0" col="0" rowspan="1" colspan="1"/>
        </cl:output>
        <cl:fileinput std:sid="f-input-4cf26a0d" id="fit3" limitfileunit="kb" limitfilesize="100">
